<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abdik.helloworld">

    <uses-permission android:name="android.permission.INTERNET" />

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Main2Activity" />
        <activity
            android:name=".happyActivity"
            android:label="@string/title_activity_happy"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".okay"
            android:label="@string/title_activity_okay"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".panic"
            android:label="@string/title_activity_panic"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".furious"
            android:label="@string/title_activity_furious"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>